{
  "name": "elysiajs-app",
  "version": "0.1.0",
  "module": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch index.js",
    "build": "bun install && bun prisma generate",
    "build:exe": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server ./index.js",
    "build:binary": "bun build --compile --minify-whitespace --minify-syntax --target bun-linux-x64 --outfile server ./index.js",
    "start:prod": "cross-env NODE_ENV=production bun ./index.js"
  },
  "devDependencies": {
    "@types/bun": "^1.2.12",
    "cross-env": "^10.1.0",
    "prisma": "^6.7.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.3.0",
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "^1.3.0",
    "@elysiajs/jwt": "^1.3.0",
    "@elysiajs/swagger": "^1.3.0",
    "@prisma/client": "^6.7.0",
    "elysia": "^1.3.0",
    "elysia-rate-limit": "^3.2.2",
    "logixlysia": "^3.1.0"
  }
}
